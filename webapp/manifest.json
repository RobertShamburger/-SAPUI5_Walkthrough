{
    "_version": "1.21.0",
    "sap.app": {
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "invoiceRemote": {
                "settings": {
                    "odataVersion": "2.0"
                },
                "type": "OData",
                "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/"
            }
        },
        "description": "{{appDescription}}",
        "i18n": {
            "bundleUrl": "i18n/i18n.properties",
            "fallbackLocale": "",
            "supportedLocales": [""]
        },
        "id": "sap.ui.demo.walkthrough",
        "title": "{{appTitle}}",
        "type": "application"
    },
    "sap.ui": {
        "deviceTypes": {
            "desktop": true,
            "phone": true,
            "tablet": true
        },
        "technology": "UI5"
    },
    "sap.ui5": {
        "rootView": {
            "id": "app",
            "type": "XML",
            "viewName": "sap.ui.demo.walkthrough.view.App"
        },
        "dependencies": {
            "libs": {
                "sap.m": {}
            },
            "minUI5Version": "1.90"
        },
        "resources": {
            "css": [{
                    "uri": "css/style.css"
                }
            ]
        },
        "models": {
            "i18n": {
                "settings": {
                    "bundleName": "sap.ui.demo.walkthrough.i18n.i18n",
                    "fallbackLocale": "",
                    "supportedLocales": [""]
                },
                "type": "sap.ui.model.resource.ResourceModel"
            },
            "invoice": {
                "dataSource": "invoiceRemote"
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "sap.ui.demo.walkthrough.view",
                "controlId": "app",
                "controlAggregation": "pages"
            },

            "routes": [{
                    "pattern": "",
                    "name": "overview",
                    "target": "overview"
                }, {
                    "pattern": "detail",
                    "name": "detail"
                }
            ],
            "targets": {
                "detail": {
                    "viewId": "detail",
                    "viewName": "Detail"
                },
                "overview": {
                    "viewId": "overview",
                    "viewName": "Overview"
                }
            }
        }
    }
}
