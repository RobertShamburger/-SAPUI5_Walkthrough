<mvc:View
	controllerName=""
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
>
	<Page
		id="employeeListPage"
		title="{i18n>EmployeeList}"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding"
	>
		<content>
			<List
				id="employeeList"
				headerText="{i18n>ListofAllEmployees}"
				items="{/Employees}"
			>
				<items>
					<StandardListItem
						title="{FirstName} {LastName}"
						iconDensityAware="false"
						iconInset="false"
					/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>